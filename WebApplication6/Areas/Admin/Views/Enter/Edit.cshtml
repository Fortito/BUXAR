@model Enter
@{
	var catagories = ViewBag.catagories as List<Catagory>;
}
<style>
	select[multiple] {
		min-height: 150px;
	}
</style>

<form method="post" enctype="multipart/form-data">
	<div class="form-group">
		<label asp-for="Title"></label>
		<input type="text" asp-for="Title" class="form-control" required />
		<span asp-validation-for="Title" class="text-danger"></span>
	</div>
	<div class="form-group">
		<label asp-for="SubTitle"></label>
		<textarea asp-for="SubTitle" class="form-control" required></textarea>
		<span asp-validation-for="SubTitle" class="text-danger"></span>
	<div class="form-group">
		<img src="@Model.PhotoUrl" width="200" alt="" />
	</div>

	<div class="form-group my-4">
		<input type="file" action="image/*" name="file" class="form-control"  />
	</div>
	<button type"submit" class="btn btn-dark">Create</button>
</form>
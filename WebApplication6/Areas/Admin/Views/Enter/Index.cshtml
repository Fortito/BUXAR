@model IEnumerable<Enter>

<a href="/admin/enter/create" class="btn btn-dark mb-3">Create</a>

<table class="table table-dark table-striped table-bordered table-hover">
    <thead>
        <tr>
            <th>#</th>
            <th>Title</th>
            <th>SubTitle</th>
            <th>Image</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Title</td>
                <td>@item.SubTitle</td>
                <td>
                    <img src="@item.PhotoUrl" width="200" alt="Image" />
                </td>
                <td>
                    <a href="/admin/enter/edit/@item.Id" class="btn btn-warning">Edit</a>
                    <a href="/admin/enter/delete/@item.Id" class="btn btn-danger">Delete</a>
                </td>
                
            </tr>
        } 
    </tbody>
</table>
